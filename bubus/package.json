{
  "name": "bubus",
  "version": "0.1.0",
  "description": "A TypeScript event bus implementation compatible with Python bubus",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-test-coverage --test test/**/*.test.js",
    "lint": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "event",
    "eventbus",
    "pubsub",
    "async"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "uuidv7": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}