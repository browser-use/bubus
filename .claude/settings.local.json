      "Bash(.venv/bin/pytest tests/ -x)",
      "Bash(.venv/bin/pytest tests/ -v --tb=short)",
      "Bash(env BUBUS_LOG_LEVEL=DEBUG timeout 20 .venv/bin/python:*)"
